def groovyUtils = new com.eviware.soapui.support.GroovyUtils( context )
def path = groovyUtils.projectPath + File.separator + "results"

def writer = new JsonBuilder()
def parser = new JsonSlurper()
def date = new Date()

def summary = parser.parseText(context.expand('${get summary#Response}'))
summary.items.each {
	def detailId = it.href.tokenize('/').pop()
	testRunner.testCase.setPropertyValue('detail_id', detailId)
	testRunner.runTestStepByName('get detail')

	def detail = parser.parseText(context.expand('${get detail#Response}'))
	it.detail = detail
}

def resultFile = new File(path + File.separator + "result_" + date.format("YYYYMMdd_HHmmss") + ".json")
resultFile.write(JsonOutput.prettyPrint(JsonOutput.toJson(summary)), "UTF-8")</script></con:config></con:testStep><con:testStep type="restrequest" name="delete" id="a794d1d3-7cbe-4ca6-94f1-acfca5fdb806"><con:settings/><con:config service="http://localhost:5000" resourcePath="/scans/{id}" methodName="" xsi:type="con:RestRequestStep" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"><con:restRequest name="delete" id="129dd832-c8b1-4862-9ecd-6bfd177512f1" mediaType="application/json" postQueryString="false"><con:settings><con:setting id="com.eviware.soapui.impl.wsdl.WsdlRequest@request-headers">&lt;xml-fragment/></con:setting></con:settings><con:endpoint>http://localhost:5000</con:endpoint><con:request/><con:originalUri>http://localhost/scans/</con:originalUri><con:assertion type="Invalid HTTP Status Codes" id="b11a4848-b8ff-4491-98e2-1591f35db7f1" name="Invalid HTTP Status Codes"><con:configuration><codes>404</codes></con:configuration></con:assertion><con:assertion type="Valid HTTP Status Codes" id="9c5eee8d-ab7b-4cc4-9c88-22f43eb53e6c" name="Valid HTTP Status Codes"><con:configuration><codes>200</codes></con:configuration></con:assertion><con:credentials><con:authType>No Authorization</con:authType></con:credentials><con:jmsConfig JMSDeliveryMode="PERSISTENT"/><con:jmsPropertyConfig/><con:parameters><entry key="id" value="${#TestCase#scan_id}" xmlns="http://eviware.com/soapui/config"/></con:parameters></con:restRequest></con:config></con:testStep><con:testStep type="restrequest" name="get detail" id="615c5e5d-7de1-437a-93b7-abeee694a99c"><con:settings/><con:config service="http://localhost:5000" resourcePath="/scans/{id}/kb/{detail_id}" methodName="" xsi:type="con:RestRequestStep" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"><con:restRequest name="get detail" id="53045d0e-2819-4bb8-9a68-21aa322568de" mediaType="application/json"><con:settings><con:setting id="com.eviware.soapui.impl.wsdl.WsdlRequest@request-headers">&lt;xml-fragment/></con:setting></con